<?xml version="1.0" encoding="utf-8"?>

<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/graph_auth"
    app:startDestination="@id/authFragment">

    <fragment
        android:id="@+id/authFragment"
        android:name="glebova.rsue.countwater.AuthFragment"
        android:label="fragment_auth"
        tools:layout="@layout/fragment_auth">
        <action
            android:id="@+id/action_authFragment_to_bottomNavFragment2"
            app:destination="@id/bottomNavFragment2" />
        <action
            android:id="@+id/action_authFragment_to_graph_new_login"
            app:destination="@id/graph_new_login" />
        <action
            android:id="@+id/action_authFragment_self"
            app:destination="@id/authFragment" />
    </fragment>
    <fragment
        android:id="@+id/bottomNavFragment2"
        android:name="glebova.rsue.countwater.ui.containers.BottomNavFragment"
        android:label="BottomNavFragment"
        tools:layout="@layout/fragment_nav_container" />
    <include app:graph="@navigation/graph_new_login" />
</navigation>